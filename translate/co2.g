#   code for maximals of Co2 
get_standard_gens_co2@:=function(P)
#  /out: P is a random process for a group isomorphic to co2 /out: Use
#  algorithm from Birmingham atlas 
local a,b,count,ord,x,y;
  repeat
    x:=Random(P);
    ord:=Order(x);
    
  until ord in Set([16,18,28]);
  a:=x^(QuoInt(ord,2));
  Unbind(x);
  repeat
    repeat
      y:=Random(P);
      ord:=Order(y);
      
    until ord in Set([15,30]);
    y:=y^(QuoInt(ord,5));
    count:=0;
    repeat
      count:=count+1;
      b:=y^Random(P);
      ord:=Order(a*b);
      
    until count=35 or ord=28;
    if ord<>28 then
      ord:=0;
      continue;
    fi;
    ord:=Order(a*b^2);
    if ord=15 then
      continue;
    fi;
    Assert(1,ord=9);
    
  until ord=9;
  Unbind(y);
  return rec(val1:=a,
    val2:=b);
end;
;

DeclareGlobalFunction("SuzIdentify@");

#  
#  Code for maximal subgroups of almost simple groups with socle the
#  sporadic group Suz.
#  Data from Rob Wilson's online Atlas.

get_standard_gens_Suz@:=function(G)
#  /out: G is a group iso to Suz /out: standard gens as defined in B'ham Atlas
#  /out: coded by Bill Unger, 3/9/200r73 
local P,a,ab,b,got_a,got_b,ord,x;
  P:=RandomProcess(G);
  got_a:=false;
  got_b:=false;
  repeat
    x:=Random(P);
    ord:=Order(x);
    if not got_a and ord=14 then
      #   1 in 28
      a:=x^7;
      got_a:=true;
      continue;
    fi;
    if not got_b and ord in Set([9,18]) then
      #   about 1 in 7
      b:=x^(QuoInt(ord,3));
      got_b:=true;
      continue;
    fi;
    
  until got_a and got_b;
  #   get a good b 
  repeat
    b:=b^Random(P);
    ab:=a*b;
    
  until Order(ab)=13 and Order(ab*ab*b)=15;
  #   about 1 in 40
  return rec(val1:=a,
    val2:=b);
end;
;
